page {
  min-height: 100%;
  background-color: #f0f0f0;
}

.none {
  display: none !important;
}

.blue {
  color: #0586cf !important;
}

.red {
  color: #fa5a69 !important;
}

.gray {
  color: #8c8c8c !important;
}

.blueBtn {
  background-color: #0586cf;
  color: #fff;
  height: 80rpx;
  border-radius: 80rpx;
  font-size: 30rpx;
}
.redBtn{
  background-color: #dc3250;
  color: #fff;
  height: 80rpx;
  border-radius: 80rpx;
  font-size: 30rpx;
}

.homeNone {
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  flex-direction: column;
  height: 100vh;
  background-color: #f0f0f0;
}

.homeNone image {
  width: 201rpx;
  height: 200rpx;
  margin-bottom: 20rpx;
}

.homeNone view {
  font-size: 30rpx;
  color: #8c8c8c;
  text-align: center;
  line-height: 1.5;
}

.homeNone button {
  margin-top: 30rpx;
  margin-bottom: 10rpx;
  width: 350rpx;
  font-size: 30rpx;
  color: #fff;
  background-color: #0084ff;
  border-radius: 10rpx;
}

.header {
  padding: 0 !important;
  position: relative;
}

.header>image {
  display: block;
  width: 100%;
  height: 360rpx;
}

.header .headImage {
  position: absolute;
  top: 70rpx;
  left: 40rpx;
  width: 150rpx;
  height: 150rpx;
  border: 5rpx solid #fff;
  border-radius: 50%;
  overflow: hidden;
}

.header .headImage image {
  width: 150rpx;
  height: 150rpx;
}

.header .headDetail {
  position: absolute;
  top: 70rpx;
  left: 230rpx;
  width: 450rpx;
}

.headDetail .f36 {
  color: #fff;
  font-weight: bold;
}
.headDetail .f36 view{
  color: #fff;
  display: inline;
  font-weight: normal;
}

.headDetail .msg {
  height: 60rpx;
  padding-top: 10rpx;
}

.headDetail .msg view {
  display: inline-block;
  overflow: hidden;
  color: #ff8c00;
  height: 40rpx;
  line-height: 40rpx;
  padding: 0 10rpx;
  background-color: #fff;
  border-radius: 40rpx;
}

.msgList {
  background-color: #fff;
}

.msgList>view {
  padding-left: 20rpx;
  padding-right: 20rpx;
}

.msgList .black, .msgList .black view {
  line-height: 1.4;
  padding: 30rpx 0;
  width: 100%;
  height: auto;
}

.productLists image {
  display: block;
  width: 200rpx;
  height: 110rpx;
}

.productLists>view {
  padding: 20rpx 0;
  border-bottom: 1px solid #e7e7e7;
  display: flex;
  justify-content: space-between;
}

.productLists>view>view {
  display: flex;
  flex: 1;
  padding-left: 30rpx;
  flex-direction: column;
  justify-content: space-between;
}

.productLists .name, .productList .name {
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  display: -webkit-box;
  -webkit-line-clamp: 1;
}

.productLists .brief, .productList .brief {
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  display: -webkit-box;
  -webkit-line-clamp: 2;
}

.serve image {
  width: 100%;
  height: 200rpx;
}

.serve>view {
  padding: 20rpx;
  border-radius: 20rpx;
  box-shadow: 1px 1px 1px 1px #d3d3d3;
  margin: 0 auto 30rpx;
  height: 260rpx;
}

.serve>view>view {
  display: flex;
  justify-content: center;
}

.serve .f24 {
  flex: 1;
  overflow: hidden;
  height: 60rpx;
}

.serve .f28 {
  padding-top: 20rpx;
}

.serve swiper {
  height: 60rpx;
  line-height: 60rpx;
}

.clientTitle {
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #fff;
  border-top: 1px solid #e7e7e7;
}

.wrap .clientTitle view {
  box-sizing: border-box;
  width: 50%;
  height: 90rpx;
  line-height: 90rpx;
  color: #282828;
  border-bottom: 1px solid #e7e7e7;
  text-align: center;
}

.wrap .clientTitle .active {
  color: #337ee2;
  border-bottom: 4rpx solid #337ee2;
}

.clientList, .clientReport {
  background-color: #fff;
}

.clientList>view, .clientReport>view {
  box-sizing: border-box;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: space-around;
  border-bottom: 1px solid #e7e7e7;
  margin: 0 20rpx;
}

.wrap .clientList view view {
  width: 33%;
  text-align: center;
}

.wrap .clientReport view view {
  width: 50%;
  text-align: center;
}

.wrap .clientList view.t, .wrap .clientReport view.t {
  color: #282828;
}

.clientReport .b {
  color: #337ee2;
}

.wrap {
  padding-bottom: 20rpx;
}

.bgWhite {
  background-color: white;
}

.wrap view {
  color: #282828;
}

.bottomBor {
  border-bottom: 20rpx solid #f0f0f0;
}

.bottomLine {
  border-bottom: 1px solid #e7e7e7;
}

.p20 {
  padding: 0 20rpx;
}

.f18 {
  font-size: 18rpx;
}

.f20 {
  font-size: 20rpx;
}

.f22 {
  font-size: 22rpx;
}

.f24 {
  font-size: 24rpx;
}

.f26 {
  font-size: 26rpx;
}

.f28 {
  font-size: 28rpx;
}

.f30 {
  font-size: 30rpx;
}

.f36 {
  font-size: 36rpx;
}

.h80 {
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
}

.h90 {
  height: 90rpx;
  line-height: 90rpx;
}

.b {
  font-weight: bold;
}

.bt20 {
  border-top: 20rpx solid #f0f0f0;
}

.w50 {
  width: 50%;
}
